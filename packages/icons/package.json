{
  "name": "@antdv-next/icons",
  "type": "module",
  "version": "1.0.0-beta.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://git.lingyu.org.cn/antdv-next/ui.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./all": {
      "types": "./dist/icons/index.d.mts",
      "import": "./dist/icons/index.mjs",
      "default": "./dist/icons/index.mjs"
    },
    "./icon/*": {
      "types": "./dist/icons/*.d.mts",
      "import": "./dist/icons/*.mjs",
      "default": "./dist/icons/*.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "test": "vitest run",
    "build": "tsdown",
    "gen:icons": "esno ./scripts/gen-icons.ts",
    "prepublish": "pnpm build",
    "bump": "bumpp --commit \"chore(release): @antdv-next/icons %s\" --push --tag \"@antdv-next/icons@%s\""
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@ant-design/colors": "catalog:prod",
    "@ant-design/icons-svg": "catalog:prod",
    "@v-c/util": "catalog:vc",
    "es-toolkit": "catalog:prod"
  },
  "devDependencies": {
    "pkg-types": "catalog:prod"
  }
}
