{
  "name": "antdv-next",
  "type": "module",
  "version": "1.0.0-alpha.1",
  "description": "Ant Design Vue3 Component Library",
  "author": {
    "email": "aibayanyu@gamil.com",
    "name": "aibayanyu20"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./dist/locale/*": {
      "types": "./dist/locale/*.d.ts",
      "import": "./dist/locale/*.js",
      "default": "./dist/locale/*.js"
    },
    "./locale/*": {
      "types": "./dist/locale/*.d.ts",
      "import": "./dist/locale/*.js",
      "default": "./dist/locale/*.js"
    },
    "./global.d.ts": "./global.d.ts",
    "./global": "./global.d.ts",
    "./dist/reset.css": "./dist/reset.css",
    "./package.json": "./package.json"
  },
  "unpkg": "dist/antd.js",
  "jsdelivr": "dist/antd.js",
  "files": [
    "dist",
    "global.d.ts",
    "package.json"
  ],
  "scripts": {
    "test": "vitest run",
    "build": "run-s build:esm build:umd",
    "build:esm": "tsdown",
    "build:umd": "vite build",
    "prepublish": "pnppm build",
    "bump": "bumpp --commit \"chore(release): antdv-next %s\" --push --tag \"antdv-next@%s\""
  },
  "dependencies": {
    "@ant-design/colors": "catalog:",
    "@ant-design/fast-color": "catalog:",
    "@antdv-next/cssinjs": "workspace:^",
    "@antdv-next/icons": "workspace:^",
    "@v-c/async-validator": "catalog:",
    "@v-c/cascader": "catalog:",
    "@v-c/checkbox": "catalog:",
    "@v-c/collapse": "catalog:",
    "@v-c/color-picker": "catalog:",
    "@v-c/dialog": "catalog:",
    "@v-c/drawer": "catalog:",
    "@v-c/dropdown": "catalog:",
    "@v-c/image": "catalog:",
    "@v-c/input": "catalog:",
    "@v-c/input-number": "catalog:",
    "@v-c/mentions": "catalog:",
    "@v-c/menu": "catalog:",
    "@v-c/mutate-observer": "catalog:",
    "@v-c/notification": "catalog:",
    "@v-c/pagination": "catalog:",
    "@v-c/picker": "catalog:",
    "@v-c/progress": "catalog:",
    "@v-c/qrcode": "catalog:",
    "@v-c/rate": "catalog:",
    "@v-c/resize-observer": "catalog:",
    "@v-c/segmented": "catalog:",
    "@v-c/select": "catalog:",
    "@v-c/slick": "catalog:",
    "@v-c/slider": "catalog:",
    "@v-c/steps": "catalog:",
    "@v-c/switch": "catalog:",
    "@v-c/table": "catalog:",
    "@v-c/tabs": "catalog:",
    "@v-c/textarea": "catalog:",
    "@v-c/tooltip": "catalog:",
    "@v-c/tour": "catalog:",
    "@v-c/tree": "catalog:",
    "@v-c/tree-select": "catalog:",
    "@v-c/trigger": "^1.0.2",
    "@v-c/upload": "catalog:",
    "@v-c/util": "catalog:",
    "@v-c/virtual-list": "catalog:",
    "@vueuse/core": "catalog:",
    "dayjs": "catalog:",
    "defu": "catalog:",
    "es-toolkit": "catalog:",
    "scroll-into-view-if-needed": "catalog:"
  }
}
